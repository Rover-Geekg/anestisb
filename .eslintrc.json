{
  "extends": "standard",
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-uses-vars": "warn",
    "react/jsx-uses-react": "warn",
    "no-multiple-empty-lines": [1, {"max": 2}],
    "padded-blocks": 0
  },
  "plugins": ["react"]
}
