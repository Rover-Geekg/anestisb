{
  "name": "upfront-editable",
  "description": "Friendly contenteditable API",
  "version": "0.0.0-development",
  "dependencies": {
    "bowser": "0.7.3",
    "rangy": "^1.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.8.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^5.0.1",
    "eslint-plugin-standard": "^1.3.2",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.1",
    "jasmine-core": "^2.2.0",
    "jquery": "^2.2.3",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.8",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-webpack": "^1.7.0",
    "load-grunt-tasks": "^1.0.0",
    "normalize.css": "^4.1.1",
    "open-browser-webpack-plugin": "0.0.2",
    "prismjs": "^1.4.1",
    "react": "^0.13.3",
    "semantic-release": "^4.3.5",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "src",
    "dist"
  ],
  "keywords": [
    "contenteditable",
    "editable"
  ],
  "license": "MIT",
  "main": "src/core.js",
  "peerDependencies": {
    "jquery": "^2.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/upfrontIO/editable.js.git"
  },
  "scripts": {
    "start": "webpack-dev-server --inline --hot",
    "prebuild:lib": "rimraf ./dist",
    "build:lib": "BUILD_DIST=true webpack",
    "prebuild:docs": "rimraf ./examples/dist",
    "build:docs": "BUILD_DOCS=true webpack",
    "build": "npm run build:lib -s && npm run build:docs -s",
    "test": "npm run build -s && npm run test:karma -s",
    "test:all": "npm run test:karma -s -- --browsers Chrome,Firefox,Safari",
    "test:karma": "karma start",
    "test:watch": "npm run test:karma -s -- --single-run false",
    "lint": "eslint .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
